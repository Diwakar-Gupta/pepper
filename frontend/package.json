{
  "name": "pep",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://diwakar-gupta.github.io/pepper/",
  "scripts": {
    "start": "parcel index.html --public-url /pepper/",
    "build": "parcel build index.html --public-url /pepper/",
    "deploy": "yarn build && cp dist/index.html dist/404.html && yarn gh-pages -d dist",
    "deploy:win": "yarn build && copy dist\\index.html dist\\404.html && yarn gh-pages -d dist",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "parcel": "^2.15.4",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss": "^8.4.33",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@uiw/react-markdown-preview": "^4.1.13",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-youtube": "^10.1.0"
  },
  "staticFiles": {
    "staticPath": "public"
  }
}
